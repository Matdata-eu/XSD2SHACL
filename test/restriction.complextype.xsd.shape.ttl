@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/NodeShape/Norwegian_customer> a sh:NodeShape ;
    sh:name "Norwegian_customer" ;
    sh:property <http://example.com/PropertyShape/Norwegian_customer/country>,
        <http://example.com/PropertyShape/Norwegian_customer/firstname>,
        <http://example.com/PropertyShape/Norwegian_customer/lastname> .

<http://example.com/NodeShape/customer> a sh:NodeShape ;
    sh:name "customer" ;
    sh:property <http://example.com/PropertyShape/customer/country>,
        <http://example.com/PropertyShape/customer/firstname>,
        <http://example.com/PropertyShape/customer/lastname> .

<http://example.com/PropertyShape/Norwegian_customer/country> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:hasValue "Norway" ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "country" ;
    sh:order 2 ;
    sh:path <http://example.com/country> ;
    sh:targetSubjectsOf <http://example.com/country> .

<http://example.com/PropertyShape/Norwegian_customer/firstname> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "firstname" ;
    sh:order 0 ;
    sh:path <http://example.com/firstname> ;
    sh:targetSubjectsOf <http://example.com/firstname> .

<http://example.com/PropertyShape/Norwegian_customer/lastname> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "lastname" ;
    sh:order 1 ;
    sh:path <http://example.com/lastname> ;
    sh:targetSubjectsOf <http://example.com/lastname> .

<http://example.com/PropertyShape/customer/country> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "country" ;
    sh:order 2 ;
    sh:path <http://example.com/country> ;
    sh:targetSubjectsOf <http://example.com/country> .

<http://example.com/PropertyShape/customer/firstname> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "firstname" ;
    sh:order 0 ;
    sh:path <http://example.com/firstname> ;
    sh:targetSubjectsOf <http://example.com/firstname> .

<http://example.com/PropertyShape/customer/lastname> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "lastname" ;
    sh:order 1 ;
    sh:path <http://example.com/lastname> ;
    sh:targetSubjectsOf <http://example.com/lastname> .

