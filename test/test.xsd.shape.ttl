@prefix : <http://tempuri.org/po.xsd> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/NodeShape/students> a sh:NodeShape ;
    sh:name "students" ;
    sh:node <http://example.com/NodeShape/student> ;
    sh:targetClass :students ;
    sh:targetObjectsOf :students .

<http://example.com/NodeShape/student> a sh:NodeShape ;
    sh:name "student" ;
    sh:property <http://example.com/PropertyShape/ID>,
        <http://example.com/PropertyShape/Name>,
        <http://example.com/PropertyShape/Sport> ;
    sh:targetClass :student ;
    sh:targetObjectsOf :student .

<http://example.com/PropertyShape/ID> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "ID" ;
    sh:path :ID ;
    sh:targetSubjectsOf :ID .

<http://example.com/PropertyShape/Name> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Name" ;
    sh:path :Name ;
    sh:targetSubjectsOf :Name .

<http://example.com/PropertyShape/Sport> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Sport" ;
    sh:path :Sport ;
    sh:targetSubjectsOf :Sport .

