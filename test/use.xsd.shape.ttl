@prefix : <http://tempuri.org/po.xsd> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/NodeShape/students> a sh:NodeShape ;
    sh:name "students" ;
    sh:property <http://example.com/PropertyShape/students/ID>,
        <http://example.com/PropertyShape/students/Name>,
        <http://example.com/PropertyShape/students/Value> ;
    sh:targetClass :students ;
    sh:targetObjectsOf :students .

<http://example.com/PropertyShape/students/ID> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "ID" ;
    sh:path :ID ;
    sh:targetSubjectsOf :ID .

<http://example.com/PropertyShape/students/Name> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Name" ;
    sh:path :Name ;
    sh:targetSubjectsOf :Name .

<http://example.com/PropertyShape/students/Value> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:name "Value" ;
    sh:path :Value ;
    sh:targetSubjectsOf :Value .

