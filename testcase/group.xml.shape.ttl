@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/NodeShape/order> a sh:NodeShape ;
    sh:name "order" ;
    sh:node <http://example.com/NodeShape/ordertype> ;
    sh:targetClass <http://example.com/order> ;
    sh:targetObjectsOf <http://example.com/order> .

<http://example.com/NodeShape/custGroup> a sh:NodeShape ;
    sh:name "custGroup" ;
    sh:property <http://example.com/PropertyShape/billto>,
        <http://example.com/PropertyShape/customer>,
        <http://example.com/PropertyShape/orderdetails>,
        <http://example.com/PropertyShape/shipto> .

<http://example.com/NodeShape/ordertype> a sh:NodeShape ;
    sh:name "ordertype" ;
    sh:node <http://example.com/NodeShape/custGroup> ;
    sh:property <http://example.com/PropertyShape/billto>,
        <http://example.com/PropertyShape/customer>,
        <http://example.com/PropertyShape/orderdetails>,
        <http://example.com/PropertyShape/shipto>,
        <http://example.com/PropertyShape/status> .

<http://example.com/PropertyShape/status> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:name "status" ;
    sh:path <http://example.com/status> ;
    sh:targetSubjectsOf <http://example.com/status> .

<http://example.com/PropertyShape/billto> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "billto" ;
    sh:order 2 ;
    sh:path <http://example.com/billto> ;
    sh:targetSubjectsOf <http://example.com/billto> .

<http://example.com/PropertyShape/customer> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "customer" ;
    sh:order 0 ;
    sh:path <http://example.com/customer> ;
    sh:targetSubjectsOf <http://example.com/customer> .

<http://example.com/PropertyShape/orderdetails> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "orderdetails" ;
    sh:order 1 ;
    sh:path <http://example.com/orderdetails> ;
    sh:targetSubjectsOf <http://example.com/orderdetails> .

<http://example.com/PropertyShape/shipto> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "shipto" ;
    sh:order 3 ;
    sh:path <http://example.com/shipto> ;
    sh:targetSubjectsOf <http://example.com/shipto> .

