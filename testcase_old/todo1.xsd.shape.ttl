@prefix : <http://tempuri.org/po.xsd> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.com/NodeShape/internationalPrice> a sh:NodeShape ;
    sh:datatype xsd:decimal ;
    sh:name "internationalPrice" ;
    sh:property <http://example.com/PropertyShape/currency> ;
    sh:targetClass :internationalPrice ;
    sh:targetObjectsOf :internationalPrice .

<http://example.com/PropertyShape/currency> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:name "currency" ;
    sh:path :currency ;
    sh:targetSubjectsOf :currency .

