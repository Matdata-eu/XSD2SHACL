era-sh:trackShape sh:sparql era-sh:MinAxleLoadVehicleCategorySKOS .
era-sh:MinAxleLoadVehicleCategorySKOS
	a sh:SPARQLConstraint ;
	rdfs:comment "Represents information that indicates the load given in tons depending of the category of vehicle. Its properties are minAxleLoadVehicleCategory and minAxleLoad.";
	era:rinfIndex "1.1.1.3.7.11.1" ;
	sh:nodeKind sh:IRI ;
	sh:severity sh:Violation ;
	sh:message "Min axle load vehicle category (1.1.1.3.7.11.1): The train detection system {$this} (label {?tdsLabel}) has a value of vehicle category {?concept} that is not one of the predefined values and cannot be converted into a SKOS concept on this list: http://data.europa.eu/949/concepts/min-axle-load-vehicle-categories/MinAxleLoadVehicleCategories."@en ;
    sh:prefixes era:;
	sh:select """
    PREFIX era: <http://data.europa.eu/949/>
    PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
    SELECT $this ?tds ?concept ?tdsLabel
    WHERE {
        OPTIONAL {$this rdfs:label ?tdsLabel} .
 		$this era:tdsMinAxleLoadVehicleCategory ?tdsMinAxleLoadVehicleCategory .
        ?tdsMinAxleLoadVehicleCategory era:minAxleLoadVehicleCategory ?concept .
        era:minAxleLoadVehicleCategory era:inSkosConceptScheme ?conceptScheme .
		FILTER NOT EXISTS{         
  			?concept skos:inScheme ?conceptScheme .
		}
	}
""" .

